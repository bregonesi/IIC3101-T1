<%= bootstrap_form_for(news) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <%= f.text_field :headline, autofocus: true, autocomplete: "off" %>
  <%= f.text_area :subhead, autocomplete: "off", rows: "3" %>
  <%= f.text_area :copy, autocomplete: "off", rows: "8" %>

  <%= f.submit class: "btn btn-success center-block" %>
<% end %>
